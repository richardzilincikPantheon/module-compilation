  "configurations": [
    {
      "name": "compile_modules - RFCStandard",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/modules_compilation/compile_modules.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "--rfc",
        "--metadata",
        "RFC-produced YANG models: Oh gosh, not all of them correctly passed pyang 2.5.3 with --ietf :-( ",
        "--forcecompilation"
      ]
    },
    {
      "name": "compile_modules - IETFDraft",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/modules_compilation/compile_modules.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "--draft",
        "--forcecompilation"
      ]
    },
    {
      "name": "compile_modules - IETFDraftExample",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/modules_compilation/compile_modules.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "--example",
        "--forcecompilation"
      ]
    },
    {
      "name": "compile_modules - IANAStandard",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/modules_compilation/compile_modules.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "--metadata",
        "IANA: Standard YANG Data Models compilation from https://github.com/YangModels/yang/tree/master/standard/iana: The 'standard/iana' branch is intended for IANA-maintained YANG models.",
        "--lint",
        "--prefix",
        "IANAStandard",
        "--rootdir",
        "/var/yang/nonietf/yangmodels/yang/standard/iana/",
        "--forcecompilation"
      ]
    },
    {
      "name": "extract_ietf_modules",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/ietf_modules_extraction/extract_ietf_modules.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false
    },
    {
      "name": "check_archived_drafts",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/check_archived_drafts.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false
    },
    {
      "name": "yang_get_stats",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/figures_and_stats/yang_get_stats.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "args": [
        "--days",
        "-1"
      ]
    }
  ]
}